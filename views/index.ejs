    <!--请在下方写此页面业务相关的脚本-->
    <script>
    function logout() {
        $.ajax({
            cache: true,
            type: "get",
            url: "/api_admin/logout",
            data: '',
            async: false,
            error: function(request) {
                console.log(request)
                $.Huimodalalert(request, 2000)
            },
            success: function(data) {
                if (data.code == 0) {
                    location.href = "/login"
                } else {
                    $.Huimodalalert(data.msg, 2000)
                }
            }
        });
    }
    </script>
